<div id="col_central">

<h2>Login entreprise</h2>


<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
  <div><%= f.label :email %><br />
  <%= f.email_field :email %></div>

  <div><%= f.label :password, "Mot de passe" %><br />
  <%= f.password_field :password %></div>

  <% if devise_mapping.rememberable? -%>
    <div><%= f.check_box :remember_me %> <%= f.label :remember_me, "MÃ©moriser mon compte" %></div>
  <% end -%>

  <div><%= f.submit "OK" %></div>
<% end %>

<%= render "links" %>

</div>
